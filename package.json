{
  "name": "npm-fund-ln",
  "version": "1.0.0",
  "description": "View funding information of your dependencies and tip them via lightning",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "lightning",
    "bitcoin",
    "alby"
  ],
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "bin": {
    "fund-ln": "./bin/fund.js"
  },
  "scripts": {
    "prebuild": "yarn run clean",
    "prepack": "yarn run build",
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "microbundle",
    "dev": "microbundle watch"
  },
  "dependencies": {
    "@npmcli/arborist": "^6.2.8",
    "alby-js-sdk": "^1.6.0",
    "alby-tools": "^2.2.1",
    "chalk": "^4.1.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "libnpmfund": "^4.0.16",
    "websocket-polyfill": "^0.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.0",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getAlby/npm-fund-ln.git"
  },
  "engines": {
    "node": ">=14"
  },
  "author": "Alby Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getAlby/npm-fund-ln/issues"
  },
  "homepage": "https://github.com/getAlby/npm-fund-ln#readme"
}
